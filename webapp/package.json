{
  "name": "mytodo-webapp",
  "description": "mytodo-webapp project",
  "version": "1.3.1",
  "license": "MIT",
  "scripts": {
    "version:update-patch": "apv init; apv update patch",
    "version:update-minor": "apv init; apv update minor",
    "build:microapp": "webpack --config ./webpack.microapp.config",
    "build:standalone": "webpack --config ./webpack.standalone.config",
    "prebuild:standalone": "rm -rf ./public/",
    "deploy": "gh-pages -d /public",
    "host:local": "npm run build:microapp && http-server",
    "production": "npm run version:update-patch && npm run build:microapp && npm run build:standalone && npm publish",
    "production-install": "npm run production && npm install",
    "transpile": "babel src -d dist --copy-files",
    "prepublishOnly": "npm run transpile",
    "dev": "webpack-dev-server --config ./webpack.dev.config.js --progress --colors",
    "dev:mocked": "mocked_bff=true npm run dev",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lintify": "eslint src --fix",
    "prettify": "prettier --check 'src/**/*.js' --write",
    "test:unit": "jest src --coverage",
    "test:unit:no-coverage": "jest src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:unit:no-coverage"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint src --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --check 'src/**/*.js' --write",
      "git add"
    ]
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.8.3",
    "@stoplight/prism-cli": "^3.2.9",
    "@storybook/react": "^5.3.18",
    "@testing-library/react-hooks": "^3.2.1",
    "@wdio/allure-reporter": "^5.18.6",
    "@wdio/cli": "^5.18.7",
    "@wdio/local-runner": "^5.22.3",
    "@wdio/mocha-framework": "^5.18.7",
    "@wdio/spec-reporter": "^5.18.7",
    "@wdio/sync": "^5.20.1",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chromedriver": "^80.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-wdio": "^5.13.2",
    "fetch-mock": "^8.0.0",
    "gh-pages": "^1.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-serve": "1.0.1",
    "husky": "^3.1.0",
    "jest": "^24.1.0",
    "jest-mock-axios": "^3.2.0",
    "jest-styled-components": "^6.3.1",
    "jsonschema": "^1.2.5",
    "lint-staged": "^9.4.3",
    "lodash": "^4.17.15",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.3",
    "terminal-log": "^1.0.1",
    "terser-webpack-plugin": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "wdio-chromedriver-service": "^5.0.2",
    "webpack": "^4.29.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "appversion": "^1.7.1",
    "axios": "^0.19.2",
    "http-server": "^0.12.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "16.8.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "single-spa": "^4.3.7",
    "single-spa-react": "^2.10.2",
    "styled-components": "^4.1.3",
    "validator": "^12.1.0"
  }
}
